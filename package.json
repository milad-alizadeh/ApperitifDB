{
  "name": "apperitivo-db",
  "version": "0.0.0",
  "description": "Minimalistic boilerplate to quick-start Node.js development in TypeScript.",
  "engines": {
    "node": ">= 18.12 <19"
  },
  "devDependencies": {
    "@types/jest": "~29.5",
    "@types/node": "~18",
    "@typescript-eslint/eslint-plugin": "~5.59",
    "@typescript-eslint/parser": "~5.59",
    "drizzle-kit": "^0.19.13",
    "eslint": "~8.38",
    "eslint-config-prettier": "~8.8",
    "eslint-plugin-jest": "~27.2",
    "husky": "^8.0.0",
    "jest": "~29.5",
    "prettier": "~2.8",
    "ts-api-utils": "~0.0.44",
    "ts-jest": "~29.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.0"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "seed": "ts-node scripts/seed.ts",
    "format": "prettier --config .prettierrc --write .",
    "lint": "eslint . --ext .ts --ext .mts",
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "migration:generate": "drizzle-kit generate:pg",
    "migration:run": "supabase db reset && yarn run seed",
    "db:staging:push": "supabase db push --include-all --debug",
    "db:local:pull": "drizzle-kit introspect:pg",
    "run:psql": "docker exec -it e36ab3141941 psql -U postgres -s postgres",
    "run:log": "docker logs -f e36ab3141941",
    "prepare": "husky install",
    "schema:push": "supabase schema push --schema public",
    "backup:staging": "pg_dump -h db.kvtfspymzqsyhmvkzgej.supabase.co -p 5432 -U postgres --data-only -f backups/backup.sql postgres",
    "restore:staging": "psql -h db.kvtfspymzqsyhmvkzgej.supabase.co -p 5432 -U postgres -d postgres --disable-triggers -f /backups/backup.sql",
    "backup:local": "pg_dump -h localhost -p 5432 -U postgres --data-only -f backups/backup.sql postgres",
    "restore:local": "psql -h localhost -p 5432 -U postgres -d postgres -f backups/backup.sql"
  },
  "author": "Jakub Synowiec <jsynowiec@users.noreply.github.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "lodash": "^4.17.21",
    "postgres": "^3.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ts-node scripts/checkChanges.js"
    }
  }
}
